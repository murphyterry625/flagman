<script type="text/javascript">
  $(document).ready( function(){   

  
    // var productUpsellId=   $('#plus-upsell-modal').data('product-upsell-id'); 


    if (window.location.hash) {
     if (window.location.hash.indexOf('gift') == 1) { 
       setTimeout(function(){   
         
         $('a.site-nav__link.site-nav__link--icon.js-drawer-open-cart .cart-link').trigger('click');
         
         console.log('click');
         window.location.hash="!";
       }, 1000);
     } 
    }
     
    
      function sendtocartproductupsell(){              
       //console.log(productUpsellId);
       // console.log('wtf');
         var productUpsellId=   $('#plus-upsell-variant option').filter(':selected').val();
		jQuery.ajax({
          	type: 'POST',
          	url: '/cart/add.js',
          	data: {
              	quantity: 1,
              	id: productUpsellId
            },
          	dataType: 'json',
          	success: function() {
              
              //	window.location.href = '/cart';
             // console.log('carrito enviado con el producto de oferta');
               $.magnificPopup.close();
              window.location.hash = 'gift';
              
             window.scrollTo(0, 0);
            location.reload();
              //$("#SiteHeader").load(location.href + " #SiteHeader");
            //  .drawer--right.drawer--is-open
            },
          
        });
    
  }
 $(document).on("click",".add-to-cart--secondary2",function() {
   sendtocartproductupsell();
    });    
   $(document).on("click",".plus-upsell-modal--close",function() {
   $.magnificPopup.close();
    }); 
   
   });
  
</script>




  
  



    

